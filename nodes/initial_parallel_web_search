from langgraph.constants import Send
from schemas import *
from prompts.risk_taxonomy import *

def initiate_parallel_web_search(state: State):
    """
    Assign each risk taxonomy to a web search node
    """
    
    # We use Send(node_name, state_for_node)
    return [Send("web_search_node", {"risk_candidate": taxonomy}) for taxonomy in RISK_TAXONOMY]